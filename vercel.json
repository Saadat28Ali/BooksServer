{ 
    "version": 2, 
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }], 
    "routes": [{
        "src": "/(.*)", 
        "dest": "api/index.js", 
        "methods": ["GET", "POST", "PATCH", "PUT", "DELETE", "OPTIONS"], 
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
    }]
}